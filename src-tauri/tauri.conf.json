{
  "build": {
    "beforeDevCommand": "yarn vite:dev",
    "beforeBuildCommand": "yarn vite:build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "JIAmusic",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "process": {
        "exit": true
      },
      "window": {
        "show": true,
        "hide": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Music",
      "publisher": "jiafeitech",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "appimage": {
        "bundleMediaFramework": true
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.github.jiafeitech.jiamusic",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "bannerPath": "installer/banner.png",
          "dialogImagePath": "installer/dialogImage.png"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "resizable": true,
        "title": "JIÎ›music",
        "width": 1288
      }
    ],
    "systemTray": {
      "iconPath": "icons/32x32.png",
      "iconAsTemplate": true
    }
  }
}
